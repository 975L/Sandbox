{
    "name": "c975l/sandbox",
    "type": "project",
    "description": "Web site sandbox for 975L.com Symfony bundles",
    "homepage": "http://sandbox.975l.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7",
        "c975L/contactform-bundle": "^1",
        "c975L/email-bundle": "^1",
        "c975L/events-bundle": "^1",
        "c975l/exceptionchecker-bundle": "^1",
        "c975L/giftvoucher-bundle": "^1",
        "c975L/includelibrary-bundle": "^1",
        "c975L/pageedit-bundle": "^1",
        "c975L/payment-bundle": "^1",
        "c975L/purchasecredits-bundle": "^1",
        "c975L/sharebuttons-bundle": "^1",
        "c975L/site-bundle": "^1",
        "c975L/toolbar-bundle": "^1",
        "c975L/user-bundle": "^1",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/doctrine-cache-bundle": "^1",
        "doctrine/orm": "^2",
        "hwi/oauth-bundle": "^0.6",
        "php-http/guzzle6-adapter": "^1",
        "php-http/httplug-bundle": "^1",
        "incenteev/composer-parameter-handler": "^2",
        "knplabs/knp-time-bundle": "^1",
        "sensio/distribution-bundle": "^5",
        "sensio/framework-extra-bundle": "^3",
        "symfony/filesystem": "^3",
        "symfony/monolog-bundle": "^3",
        "symfony/polyfill-apcu": "^1",
        "symfony/property-access": "^3",
        "symfony/serializer": "^3",
        "symfony/symfony": "^3",
        "twig/extensions": "^1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3",
        "symfony/phpunit-bridge": "^3",
        "c975L/xliff-bundle": "^1",
        "c975l/countlinescode-bundle": "^1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
