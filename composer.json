{
    "name": "c975l/sandbox",
    "type": "project",
    "description": "Web site sandbox.975l.com",
    "homepage": "https://sandbox.975l.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "c975l/contactform-bundle": "^2",
        "c975l/exceptionchecker-bundle": "^2",
        "c975l/includelibrary-bundle": "^1",
        "c975l/pageedit-bundle": "^2",
        "c975l/payment-bundle": "^2",
        "c975l/purchasecredits-bundle": "^2",
        "c975l/site-bundle": "^2",
        "c975l/user-bundle": "^2",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/doctrine-cache-bundle": "^1",
        "doctrine/orm": "^2",
        "sensio/framework-extra-bundle": "^5",
        "symfony/asset": "^4",
        "symfony/console": "^4",
        "symfony/dotenv": "^4",
        "symfony/expression-language": "^4",
        "symfony/flex": "^1",
        "symfony/form": "^4",
        "symfony/framework-bundle": "^4",
        "symfony/http-client": "^4",
        "symfony/intl": "^4",
        "symfony/monolog-bundle": "^3",
        "symfony/orm-pack": "*",
        "symfony/process": "^4",
        "symfony/security-bundle": "^4",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3",
        "symfony/translation": "^4",
        "symfony/twig-bundle": "^4",
        "symfony/validator": "^4",
        "symfony/web-link": "^4",
        "symfony/yaml": "^4",
        "twig/extensions": "^1"
    },
    "require-dev": {
        "c975l/countlinescode-bundle": "^1",
        "c975l/xliff-bundle": "^2",
        "phpstan/phpstan": "^0",
        "rector/rector": "^0",
        "symfony/debug-pack": "*",
        "symfony/phpunit-bridge": "^4",
        "symfony/maker-bundle": "^1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "^4",
        "symplify/easy-coding-standard": "^5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4"
        }
    }
}
